{% extends '@layout/default.html.twig' %}

{% block title %}{{ data }} }}{% endblock %}

{% block content %}
    {{ data }}

    {% for cliente in clientes %}
        <pre>
        {{ cliente.id }} - {{ cliente.nome }} - {{ cliente.email }} - {{ cliente.cpf }} <br>
        {% for endereco in cliente.enderecos %}
            {{ endereco.logradouro }} - {{ endereco.cidade }} - {{ endereco.estado }} - {{ endereco.cep }} <br>
        {% endfor %}
        </pre>
    {% endfor %}
{% endblock %}
